<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity LogEntry.
    -->
    <changeSet id="20190402214306-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="document_id"
                                 baseTableName="log_entry"
                                 constraintName="fk_log_entry_document_id"
                                 referencedColumnNames="id"
                                 referencedTableName="document"/>

        <addForeignKeyConstraint baseColumnNames="manual_id"
                                 baseTableName="log_entry"
                                 constraintName="fk_log_entry_manual_id"
                                 referencedColumnNames="id"
                                 referencedTableName="manual"/>

        <addForeignKeyConstraint baseColumnNames="organization_id"
                                 baseTableName="log_entry"
                                 constraintName="fk_log_entry_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="organization"/>

        <addForeignKeyConstraint baseColumnNames="section_id"
                                 baseTableName="log_entry"
                                 constraintName="fk_log_entry_section_id"
                                 referencedColumnNames="id"
                                 referencedTableName="section"/>

    </changeSet>
</databaseChangeLog>
