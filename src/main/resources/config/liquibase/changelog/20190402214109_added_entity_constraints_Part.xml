        <addForeignKeyConstraint baseColumnNames="version_id"
                                 baseTableName="part"
                                 constraintName="fk_part_version_id"
                                 referencedColumnNames="id"
                                 referencedTableName="version"/>

    </changeSet>
</databaseChangeLog>
